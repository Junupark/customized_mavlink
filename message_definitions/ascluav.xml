        <description>Category Navigation</description>
      </entry>
      <entry value="1" name="ASCL_CATEGORY_CONTROL">
        <description>Category Control</description>
      </entry>
      <entry value="2" name="ASCL_CATEGORY_GUIDANCE">
        <description>Category Guidance</description>
      </entry>
      <entry value="3" name="ASCL_CATEGORY_MISSION">
        <description>Category Mission</description>
      </entry>
      <entry value="4" name="ASCL_CATEGORY_RESERVED1">
        <description>Category reserved</description>
      </entry>
      <entry value="5" name="ASCL_CATEGORY_RESERVED2">
        <description>Category reserved</description>
      </entry>
      <entry value="6" name="ASCL_CATEGORY_RESERVED3">
        <description>Category reserved</description>
      </entry>
    </enum>
